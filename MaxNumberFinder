public class Max_no {
    public static void main(String[] args) {
        int[] arr = {12, 23, 4, 55, 7890};
        int temp;
        int max = arr[0];
        for (int i = 1; i < 5; i++) {
            if (max < arr[i]) {
                temp = arr[i];
                arr[i] = arr[0];
                max = temp;
            }
        }
            System.out.println("Maximum Number is:"+max);

        }
    }
