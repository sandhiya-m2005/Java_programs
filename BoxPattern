public class box  {
    public static void main(String[] args) {
        for(int i=0;i<10;i++){
            System.out.print("* ");
        }
        System.out.println();
        for(int i=0;i<10;i++){
            System.out.println("*                 *");
        }
        for(int i=0;i<10;i++){
            System.out.print("* ");
        }
    }
} 
